<%- include('header.ejs')%>
    <div class="container-fluid contact">
      <br><br><br>
      <div class="page-desc">
        <div class="row justify-content-center">
          <div class="col-md-8">
            <center><h1 style="font-weight: bold; color: black;">Contact US</h1></center>
            <form method="post" enctype="multipart/form-data" action="/contactinsert">
              <div class="form-group">
                <input type="text" name="name" class="form-control" placeholder="Name">
              </div>
              <div class="form-group">
                <input type="text" name="email" class="form-control" placeholder="E-Mail">
              </div>
              <div class="form-group">
                <input type="text" name="Phone" class="form-control" placeholder="Phone">
              </div>
              <div class="form-group">
                <textarea name="message" rows="5" class="form-control" placeholder="Message"></textarea>
              </div>
              <div class="form-group">
                <button class="btn btn-success">Send Message</button>
              </div>
            </form>
          </div>
        </div>
      </div>
      <br><br><br>
</div>
<%- include('footer.ejs')%>